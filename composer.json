{
    "name": "php-etl/log-plugin",
    "description": "Plugins for adding Logging support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Kiboko\\Plugin\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Plugin\\Log\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "require": {
        "php": "^8.0",
        "php-etl/configurator-contracts": "0.1.x-dev",
        "nikic/php-parser": "^4.9@dev",
        "symfony/config": "^4.0 || ^5.0"
    },
    "require-dev": {
        "symfony/yaml": "5.x-dev",
        "symfony/console": "5.x-dev",
        "phpunit/phpunit": "^9.0",
        "adlawson/vfs": "dev-develop"
    }
}
