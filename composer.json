{
    "name": "php-etl/log-plugin",
    "description": "Plugins for adding Logging support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kiboko\\Plugin\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Plugin\\Log\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.x-dev"
        }
    },
    "require": {
        "php": "^8.2",
        "php-etl/configurator-contracts": "^0.6",
        "nikic/php-parser": "^4.9@dev",
        "symfony/config": "^6.0"
    },
    "require-dev": {
        "symfony/yaml": "^6.0",
        "symfony/console": "^6.0",
        "phpunit/phpunit": "^10.0",
        "mikey179/vfsstream": "^1.6",
        "monolog/monolog": "2.x-dev",
        "elasticsearch/elasticsearch": "~7.0",
        "graylog2/gelf-php": "^1.7",
        "friendsofphp/php-cs-fixer": "^3.0",
        "rector/rector": "^0.15",
        "phpstan/phpstan": "^1.10",
        "infection/infection": "^0.26.18"
    }
}
