{
    "name": "php-etl/log-plugin",
    "description": "Plugins for adding Logging support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Kiboko\\Plugin\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Plugin\\Log\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2.x-dev"
        }
    },
    "require": {
        "php": "^8.1",
        "php-etl/configurator-contracts": "^0.5.0",
        "nikic/php-parser": "^4.9@dev",
        "symfony/config": "^4.0 || ^5.0"
    },
    "require-dev": {
        "symfony/yaml": "^5.2",
        "symfony/console": "^5.2",
        "phpunit/phpunit": "^9.0",
        "adlawson/vfs": "dev-develop",
        "monolog/monolog": "2.x-dev",
        "elasticsearch/elasticsearch": "~7.0",
        "graylog2/gelf-php": "^1.7",
        "friendsofphp/php-cs-fixer": "^3.0"
    }
}
